{
  "name": "jssm-viz-cli",
  "version": "0.6.2",
  "description": "jssm-viz-cli is a command-line interface for jssm-viz, a flowchart vizualizer for fsl finite state machines, using jssm to render FSMs. ",
  "main": "dist/js/cli.js",
  "directories": {
    "doc": "docs"
  },
  "bin": {
    "jssm-viz": "dist/src/ts/cli.js"
  },
  "scripts": {
    "clean": "rimraf -f dist/ && rimraf -f docs/ && mkdir -p dist/js docs",
    "docs": "cp src/svg/*.svg docs/",
    "typescript": "tsc --project tsconfig.json",
    "start": "node ./dist/src/ts/cli.js",
    "build": "npm run clean && npm run typescript && npm run docs",
    "test": "echo Temporarily skipped"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/jssm-viz-cli.git"
  },
  "keywords": [
    "jssm",
    "jssm-viz",
    "jssm-viz-cli",
    "cli",
    "visualize",
    "visualizer",
    "fsl",
    "finite",
    "state",
    "machine",
    "fsm",
    "state-machine"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "contributors": [
    {
      "name": "Vat Raghavan",
      "email": "machinshin@gmail.com",
      "url": "https://github.com/machinshin"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/fsl/issues"
  },
  "homepage": "https://github.com/StoneCypher/jssm-viz-cli#readme",
  "dependencies": {
    "commander": "^4.1.0",
    "jssm-viz": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^13.1.4",
    "glob": "^7.1.6",
    "rimraf": "^3.0.0",
    "sharp": "^0.23.4",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4",
    "viz.js": "^2.1.2"
  }
}
